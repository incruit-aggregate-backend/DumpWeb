<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dispatch.dump.commonModule.db.mapper.DailyReportStep7Mapper">

    <!-- 제출처 정보 저장-->
    <insert id="insertDailyReportStep7" parameterType="DailyReportStep7Main">
        insert into
        tSheet
        (date, CarNo, chk1)
        values
        (#{date}, #{CarNo}, #{chk1})

        <selectKey
                keyColumn="sheetID"
                keyProperty="sheetID"
                order="AFTER"
                resultType="int"
        >
            select last_insert_id() as sheetID
        </selectKey>

    </insert>

</mapper>